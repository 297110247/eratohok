	レミリア_最大部位 '= "Ｖ"
	レミリア_最大部位称 '= CALL_DIRTY_K10("Ｖ")
ELSEIF PALAM:レミリア_対象:快Ａ >= 10 && PALAM:レミリア_対象:快Ａ >= PALAM:レミリア_対象:快Ｃ && PALAM:レミリア_対象:快Ａ >= PALAM:レミリア_対象:快Ｖ && PALAM:レミリア_対象:快Ａ >= PALAM:レミリア_対象:快Ｂ
