@EVENT_PRISONER_TEXT0(ARG:0)

IF TALENT:(ARG:0):崩壊 || TALENT:(ARG:0):空虚
	PRINTFORML %ANAME(ARG:0)%空虚的視線在牢中彷徨
ELSE
	IF RAND:4
		IF ABL:(ARG:0):知略 >= 70
			PRINTFORML %ANAME(ARG:0)%在監牢中非常老実…
		ELSEIF ABL:(ARG:0):武闘 >= 70
			PRINTFORML %ANAME(ARG:0)%在監牢中暴乱着…
		ELSEIF ABL:(ARG:0):政治 >= 70
			PRINTFORML %ANAME(ARG:0)%和監牢外的看守搭着話…
		ELSE
			PRINTFORML %ANAME(ARG:0)%在監牢中十分不安…
		ENDIF
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML %ANAME(ARG:0)%在監牢中一動不動…
			CASE 1
				PRINTFORML %ANAME(ARG:0)%在監牢中発着牢騷…
			CASE 2
				PRINTFORML %ANAME(ARG:0)%在監牢中十分心虚…
			CASEELSE
				PRINTFORML %ANAME(ARG:0)%在監牢中十分不安…
		ENDSELECT
	ENDIF
ENDIF

