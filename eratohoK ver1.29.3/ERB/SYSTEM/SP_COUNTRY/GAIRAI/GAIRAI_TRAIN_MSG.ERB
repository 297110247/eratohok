				CALL FUCK_SP(対象, "欲望, 緊縛, 苦痛快楽, 被虐", "", 勢力番号, GET_SPERM_ID("外来人"), @"外来人的\@ RAND:2 ? 唇 # 肉棒 \@", "外来人", "")
			CASE 3
